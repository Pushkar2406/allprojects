@model User
@{ Layout = "_Layout"; ViewBag.Title = "Add a User";}

<h2>Add User <a asp-action="Index" class="btn btn-sm btn-secondary">Back</a></h2>
<form asp-action="AddUser" method="post">
    <div class="form-group">
        <label for="AddName">UserName:</label>
        <input class="form-control" name="UserName" />
    </div>
    <div class="form-group">
        <label for="Useremail">User email:</label>
        <input class="form-control" name="Useremail" />
    </div>
    <div class="text-center panel-body">
        <button type="submit" class="btn btn-sm btn-primary">Add</button>
    </div>
    </div>
</form>


